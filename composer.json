{
    "name": "zofe/rapyd",
    "description": "crud widgets for laravel, to make an admin in few lines of code",
    "homepage": "https://github.com/zofe/rapyd-laravel",
    "keywords": ["laravel", "crud", "rapyd", "administrator", "admin", "backend", "datatable", "form"],
    "authors": [
        {
            "name": "zofe",
            "email": "felice.ostuni@gmail.com"
        }
    ],
    "require": {
        "laravelcollective/html": "^6.0.0",
        "intervention/image": "2.*",
        "zofe/burp": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:mgallineo/burp.git"
        }
    ],
    "suggest": {
        "baum/baum": "Allows to use the DataTree widget"
    },
    "autoload": {
        "classmap": [
            "migrations"
        ],
        "psr-4": {
            "Zofe\\Rapyd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zofe\\Rapyd\\RapydServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
